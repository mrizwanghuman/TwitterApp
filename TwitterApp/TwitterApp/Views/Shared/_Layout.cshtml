

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>


    @Styles.Render("~/content/Bootstrap")
    @Styles.Render("~/content/main")
</head>
<body>
    <div class="container-fluid">

        <nav class="navbar navbar-default">
